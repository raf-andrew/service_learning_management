{
    "name": "service-learning-management",
    "version": "1.0.0",
    "description": "Service Learning Management System",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "test:unit": "vitest run tests/Unit/",
        "test:integration": "vitest run tests/Integration/",
        "test:e2e": "vitest run tests/E2E/",
        "test:frontend": "vitest run tests/Frontend/",
        "test:functional": "vitest run tests/Functional/",
        "test:sanity": "vitest run tests/Sanity/",
        "test:chaos": "vitest run tests/Chaos/",
        "test:ai": "vitest run tests/AI/",
        "test:mcp": "vitest run tests/MCP/",
        "test:security": "vitest run tests/Security/",
        "test:services": "vitest run tests/Unit/Services/",
        "test:models": "vitest run tests/Unit/Models/",
        "test:commands": "vitest run tests/Unit/Commands/",
        "test:sniffing": "vitest run tests/Unit/Sniffing/",
        "test:api": "vitest run tests/Functional/ --reporter=verbose",
        "test:resilience": "vitest run tests/Chaos/ --reporter=verbose",
        "test:vulnerability": "vitest run tests/Security/ --reporter=verbose",
        "test:php": "php artisan test",
        "test:php:coverage": "php artisan test",
        "test:php:unit": "php artisan test tests/Unit/",
        "test:php:feature": "php artisan test tests/Feature/",
        "test:all": "npm run test:coverage",
        "test:ci": "npm run test:coverage -- --reporter=verbose",
        "test:comprehensive": "npm run test:unit && npm run test:functional && npm run test:sanity && npm run test:security",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
        "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .gitignore",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@inertiajs/vue3": "^1.0.0",
        "@vueuse/core": "^10.0.0",
        "axios": "^1.6.0",
        "vue": "^3.3.0",
        "vue-router": "^4.2.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/vue": "^8.1.0",
        "@types/node": "^24.0.3",
        "@types/testing-library__jest-dom": "^5.14.9",
        "@vitejs/plugin-vue": "^4.5.0",
        "@vitest/coverage-v8": "^1.6.1",
        "@vitest/ui": "^1.6.1",
        "@vue/compiler-sfc": "^3.3.0",
        "@vue/test-utils": "^2.4.0",
        "autoprefixer": "^10.4.0",
        "eslint": "^8.0.0",
        "eslint-plugin-vue": "^9.0.0",
        "jsdom": "^26.1.0",
        "pinia": "^3.0.3",
        "postcss": "^8.4.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vitest": "^1.6.1"
    }
}
