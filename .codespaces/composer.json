{
    "name": "service-learning/codespaces",
    "description": "Codespaces deployment and management system",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0",
        "symfony/process": "^6.0",
        "symfony/yaml": "^6.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Codespaces\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codespaces\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyze": "phpstan analyze",
        "check-style": "phpcs",
        "fix-style": "phpcbf"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
} 