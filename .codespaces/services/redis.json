{
    "name": "redis",
    "type": "redis",
    "version": "6.2",
    "env": {
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PASSWORD": null,
        "REDIS_PORT": "6379",
        "REDIS_CLIENT": "predis"
    },
    "health_check": {
        "type": "ping",
        "timeout": 5
    },
    "persistence": {
        "enabled": true,
        "type": "aof",
        "appendonly": "yes"
    }
} 